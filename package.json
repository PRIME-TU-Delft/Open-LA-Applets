{
  "name": "open-la-applets",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:fps": "VITE_SHOW_FPS=true vite dev",
    "build": "vite build",
    "build-netlify": "pnpx puppeteer browsers install chrome@141.0.7390.76 && BUILD_ENV=netlify vite build && BUILD_ENV=netlify tsx scripts/generate-screenshots.ts",
    "build:with-screenshots": "vite build && tsx scripts/generate-screenshots.ts",
    "preview": "vite preview",
    "screenshots": "tsx scripts/generate-screenshots.ts",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/compat": "^1.4.1",
