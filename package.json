    "@cortex-js/compute-engine": "^0.30.2",
    "@number-flow/svelte": "^0.3.10",
    "@sveltejs/adapter-netlify": "^5.2.4",
    "@sveltejs/adapter-node": "^5.5.2",
    "@threlte/core": "^8.3.1",
