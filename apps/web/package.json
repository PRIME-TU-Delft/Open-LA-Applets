    "@sveltejs/adapter-node": "latest",
    "@sveltejs/kit": "latest",
    "@threlte/preprocess": "^0.0.2",
    "eslint-config-custom": "*",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "svelte-sequential-preprocessor": "^0.0.7",
    "tslib": "^2.3.1",
    "typescript": "^4.7.2",
    "vite": "^3.1.6"
  },
  "type": "module"
